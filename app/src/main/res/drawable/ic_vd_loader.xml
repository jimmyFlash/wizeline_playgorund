<?xml version="1.0" encoding="utf-8"?>
<!-- virtual canvas of 72x24 -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="72dp"
    android:height="24dp"
    android:viewportWidth="72"
    android:viewportHeight="24">

    <!-- scaleGroup which draws a circle -->
    <group
        android:name="scaleGroup"
        android:pivotX="12"
        android:pivotY="12"
        android:scaleX="1"
        android:scaleY="1">
        <path
            android:fillColor="#D50000"
            android:fillType="evenOdd"
            android:pathData="M12,12m-8,0a8,8 0,1 1,16 0a8,8 0,1 1,-16 0"
            android:strokeWidth="1"
            android:strokeColor="#00000000" />
    </group>

    <!--  translateGroup which draws a square -->
    <group
        android:name="translateGroup"
        android:translateX="1"
        android:translateY="1">
        <path
            android:fillColor="#FFAB00"
            android:fillType="evenOdd"
            android:pathData="M28,4h16v16h-16z"
            android:strokeWidth="1"
            android:strokeColor="#00000000" />
    </group>

    <!-- otationGroup which draws a triangle image -->
    <group
        android:name="rotationGroup"
        android:pivotX="60"
        android:pivotY="14"
        android:rotation="0">
        <path
            android:fillColor="#00BFA5"
            android:fillType="evenOdd"
            android:pathData="M60,4l8,16l-16,0z"
            android:strokeWidth="1"
            android:strokeColor="#00000000" />
    </group>
</vector>
